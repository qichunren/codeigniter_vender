<?php
class <%= class_name %>_model extends Model {
<% attributes.each do |attribute| -%>
  var $<%= attribute.name -%> = '';
<% end -%>
<% unless options[:skip_timestamps] -%>
  var $created_at = '';
  var $updated_at = '';
<% end -%>

  function <%= class_name %>_model(){
    parent::Model();
  }
}
?>